   "source": [
    "from scipy.stats import norm  \n",
    "mean = 4  \n",
    "std_dev = 1  \n",
    "x_value = 4.25   \n",
    "z_score = (x_value - mean) / std_dev   \n",
    "percentage_above_4_25 = norm.sf(z_score) * 100  \n",
    "print(f\"Approximately {round(percentage_above_4_25)}% of scores fall above 4.25.\")  \n"
   ]
  }
